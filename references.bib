  keywords  = {electronic countermeasures;interference suppression;jamming;missiles;radar interference;radio networks;search radar;cognitive radar network;multiplatform radar management;interference minimisation;jamming;anti-radiation missiles;radar resources;self-learning methods;air surveillance radar;Radar applications;Radar tracking;Radar countermeasures;Radar signal processing;Machine vision;Surveillance;Humans;Airborne radar;Radar detection;Aircraft},
  url       = {https://ieeexplore.ieee.org/document/4760864},
}

