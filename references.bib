@Article{Hero2011,
  author   = {A. O. {Hero} and D. {Cochran}},
  journal  = {IEEE Sensors Journal},
  title    = {{Sensor management: Past, Present, and Future}},
  year     = {2011},
  issn     = {2379-9153},
  month    = dec,
  number   = {12},
  pages    = {3064 - 3075},
  volume   = {11},
  abstract = {Sensor systems typically operate under resource constraints that prevent the simultaneous use of all resources all of the time. Sensor management becomes relevant when the sensing system has the capability of actively managing these resources; i.e., changing its operating configuration during deployment in reaction to previous measurements. Examples of systems in which sensor management is currently used or is likely to be used in the near future include autonomous robots, surveillance and reconnaissance networks, and waveform-agile radars. This paper provides an overview of the theory, algorithms, and applications of sensor management as it has developed over the past decades and as it stands today.},
  doi      = {10.1109/JSEN.2011.2167964},
  groups   = {sensor management},
  keywords = {sensors;technology management;sensor management;resource constraints;development;Robot sensing systems;Radar antennas;Sensor systems;Measurement;Radar applications;Active adaptive sensors;information-optimized planning;multiarmed bandits;multistage planning;myopic planning;optimal decision policies;plan-ahead sensing;policy approximation;radar waveform scheduling;reinforcement learning;sequential decision processes;stochastic control},
}

@InProceedings{Byrne2016,
  author    = {M. {Byrne} and K. {White} and J. {Williams}},
  booktitle = {2016 19th International Conference on Information Fusion (FUSION)},
  title     = {Rolling horizon non-myopic scheduling of multifunction radar for search and track},
  year      = {2016},
  month     = jul,
  pages     = {634-642},
  abstract  = {New developments in sensor technologies have enabled more efficient and effective application of resources. Sensors that are agile and support multiple modes of operation require adaptive scheduling approaches in order to yield performance benefits compared to fixed off-line approaches. User objectives for different types of tasks must be carefully defined in order to ensure appropriate allotment of sensor resources. Myopic scheduling approaches concentrate on present sensing actions only, while non-myopic scheduling approaches concentrate on present and future actions. In this paper, two different algorithms are developed for the rolling horizon, or multiple time step look-ahead, non-myopic scheduling problem. An example of a multifunction electronically scanned radar that is tasked to (i) search for targets and (ii) update tracks on targets is used to show how the algorithms can be applied in practice.},
  groups    = {sensor management, MFR resource management, Dwell optimization},
  keywords  = {adaptive scheduling;radar receivers;radar tracking;search radar;target tracking;multifunction radar rolling horizon nonmyopic scheduling;adaptive scheduling approach;multiple time step look-ahead nonmyopic scheduling problem;multifunction electronically scanned radar;target tracking;target searching;Radar tracking;Target tracking;Silicon;Schedules;Search problems;Surveillance;Sensor Management;Radar;Surveillance;Search;Tracking},
  url       = {https://ieeexplore.ieee.org/document/7527947},
}

@InProceedings{Qiang2017,
  author    = {X. {Qiang} and Z. {Weigang} and J. {Xin}},
  booktitle = {2017 2nd IEEE International Conference on Computational Intelligence and Applications (ICCIA)},
  title     = {Research on method of intelligent radar confrontation based on reinforcement learning},
  year      = {2017},
  month     = sep,
  pages     = {471-475},
  abstract  = {With the progress of radar technology, the development of radar tends to intellectualization, and the combat effectiveness of radar confrontation method for conventional radar is decreasing. So it is urgent to study the intelligent radar confrontation (IRC) method. Based on this, this paper expounds the intelligent radar confrontation method, and compares the difference between traditional radar confrontation (TRC) and intelligent radar confrontation. Aiming at the problem of single function and poor real-time performance in the process of conventional radar confrontation, IRC based on reinforcement learning (RL) is studied, and the formula of Q value updating in IRC process is deduced. The simulation shows that the intelligent radar jamming method can independently learn and make decision according to the jamming effect, improving the adaptability of the radar countermeasure system, and can resist the multi-working mode radar simultaneously.},
  comment   = {Learn jamming strategy with Q-learning based on the radars working mode (state). It is assumed that different jamming signals changes radar state (with different threats) and it can be used to sequantially guide to the lowest threat level.},
  doi       = {10.1109/CIAPP.2017.8167262},
  groups    = {RL in Radar, Jamming},
  keywords  = {electronic countermeasures;jamming;learning (artificial intelligence);radar computing;intelligent radar confrontation method;reinforcement learning;intelligent radar jamming method;radar countermeasure system;multiworking mode radar;radar technology;Jamming;Radar countermeasures;Real-time systems;Radar measurements;Radar equipment;Learning (artificial intelligence);intellectualization;radar confrontation;reinforcement learning;Q-Learning},
}

@Article{Haykin2006,
  author   = {S. {Haykin}},
  journal  = {IEEE Signal Processing Magazine},
  title    = {Cognitive radar: a way of the future},
  year     = {2006},
  issn     = {1558-0792},
  month    = jan,
  number   = {1},
  pages    = {30-40},
  volume   = {23},
  abstract = {This article discusses a new idea called cognitive radar. Three ingredients are basic to the constitution of cognitive radar: 1) intelligent signal processing, which builds on learning through interactions of the radar with the surrounding environment; 2) feedback from the receiver to the transmitter, which is a facilitator of intelligence; and 3) preservation of the information content of radar returns, which is realized by the Bayesian approach to target detection through tracking. All three of these ingredients feature in the echo-location system of a bat, which may be viewed as a physical realization (albeit in neurobiological terms) of cognitive radar. Radar is a remote-sensing system that is widely used for surveillance, tracking, and imaging applications, for both civilian and military needs. In this article, we focus on future possibilities of radar with particular emphasis on the issue of cognition. As an illustrative case study along the way, we consider the problem of radar surveillance applied to an ocean environment.},
  doi      = {10.1109/MSP.2006.1593335},
  groups   = {Radar basics},
  keywords = {cognitive systems;radar signal processing;radar receivers;radar transmitters;belief networks;target tracking;remote sensing by radar;oceanographic techniques;radar computing;cognitive radar;intelligent signal processing;receiver;transmitter;Bayesian approach;target detection;target tracking;echo-location system;remote-sensing system;radar surveillance;ocean environment;Radar tracking;Radar remote sensing;Radar signal processing;Radar imaging;Target tracking;Surveillance;Constitution;Transmitters;Bayesian methods;Object detection},
  url      = {https://doi.org/10.1109/MSP.2006.1593335},
}

@Article{Yang2018,
  author   = {Y. {Yang} and T. {Zhang} and W. {Yi} and L. {Kong} and X. {Li} and X. {Yang}},
  journal  = {IET Radar, Sonar Navigation},
  title    = {Multi-static radar power allocation for multi-stage stochastic task of missile interception},
  year     = {2018},
  issn     = {1751-8792},
  number   = {5},
  pages    = {540-548},
  volume   = {12},
  abstract = {Considering a multi-stage stochastic task, in which a multi-static radar system (MSRS) is applied to assist with missile interception, the authors study an optimisation problem of radar resource management. Specifically, under restriction of a fixed energy budget, the authors devote to minimise the loss, which is caused by the unsuccessfully intercepted missiles, through optimal power allocation (OPA) of MSRS within multiple stages. The design of OPA can be translated into a sequential decision-making problem. The authors formulate the problem through variable definition and modelling the missile interception procedure. As the authors need to consider the randomness of multiple coupled stages and jointly allocation power between multiple radar nodes, to solve the proposed problem is of huge computational load. The authors propose a solution that combines with reinforcement learning and particle swarm optimisation. Comparing with the uniform power allocation scheme, the simulation results demonstrate that the OPA scheme designed by the proposed method is capable to achieve preferable and more stable performance for the whole missile interception. The authors' contributions include a novel optimisation resource management model for a multi-stage stochastic task and an effective solution for the optimal resource management scheme.},
  comment  = {Missile interception modeled as a sequential decision process where radar trasmit power for multiple radars need to be allocated to minimize the loss. The loss/reward function is related to how many threaths are destoyed and how many survived assests.},
  doi      = {10.1049/iet-rsn.2017.0534},
  groups   = {RL in Radar, Parameter selection},
  keywords = {decision making;learning (artificial intelligence);military computing;military radar;minimisation;missiles;particle swarm optimisation;stochastic processes;multistatic radar power allocation;missile interception multi-stage stochastic task;MSRS;radar resource management optimisation problem;fixed energy budget;loss minimisation;optimal power allocation;OPA;sequential decision-making problem;multiple coupled stages;computational load;reinforcement learning;particle swarm optimisation;uniform power allocation scheme;optimal resource management scheme},
}

@InProceedings{Wang2019,
  author    = {Y. {Wang} and T. {Zhang} and L. {Xu} and T. {Tian} and L. {Kong} and X. {Yang}},
  booktitle = {2019 IEEE Radar Conference (RadarConf)},
  title     = {Model-free Reinforcement Learning based Multi-stage Smart Noise Jamming},
  year      = {2019},
  month     = apr,
  pages     = {1-6},
  abstract  = {In this paper, considering a multi-stage electronic countermeasure against a Fire Control Radar (FCR) with multiple working modes, an optimal multi-stage jamming power allocation for smart noise jamming is investigated with unknown environment model. The optimal jamming power allocation problem is solved by proposing a multi-stage jamming power allocation method based on model-free reinforcement learning. Firstly, we construct a Markov decision process (MDP) with unknown environment model for multi-stage jamming power allocation. To evaluate the performance of multi-stage smart noise jamming, we choose the expected of the search to lock-on time of the FCR as an evaluation criterion. Then, to overcome the challenge of the unknown environment model, a reinforcement learning framework for multi-stage jamming power allocation is formulated. Under the framework, a method of multi-stage jamming power allocation based on Q-learning is proposed. Finally, numerical results are provided to verify the validity of the proposed method.},
  comment   = {Model-free jamming system  learns to control jammer power to prevent probability of detection (because of jamming) and still achieve succesful jamming. The jamming decisions are based on the fire control radars state (mode) that is assumed to be possible to probe.},
  doi       = {10.1109/RADAR.2019.8835788},
  groups    = {RL in Radar, Jamming},
  issn      = {1097-5659},
  keywords  = {electronic countermeasures;jamming;learning (artificial intelligence);Markov processes;radar computing;model-free reinforcement learning;multistage smart noise jamming;multistage electronic countermeasure;unknown environment model;optimal jamming power allocation problem;multistage jamming power allocation method;fire control radar;Markov decision process;Jamming;Resource management;Reinforcement learning;Aircraft;Aerospace electronics;Electronic countermeasures;Numerical models},
  url       = {https://doi.org/10.1109/RADAR.2019.8835788},
}

