  groups    = {RL in Radar, Anti-jamming},
  keywords  = {cognitive radar;frequency hop communication;jamming;learning (artificial intelligence);radar computing;radar interference;cognitive radar;Q-learning;DQN;reinforcement learning algorithm;frequency agile radar;smart jammer model;radar antijamming designs;antijamming frequency hopping strategy design method;FA radar;deep Q-network;signal-to-interference-plus-noise ratio;SINR;Jamming;Radar cross-sections;Interference;Signal to noise ratio;Neural networks;Cognitive radar;cognitive radar;frequency hopping;Q-learning;deep Q-network;jammer},
